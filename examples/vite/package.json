{
  "name": "notion-formula-wasm-demo",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "typecheck": "pnpm -s run typecheck:app && pnpm -s run typecheck:tests",
    "typecheck:app": "tsc -p tsconfig.json --noEmit --pretty false",
    "typecheck:tests": "tsc -p tests/tsconfig.json --noEmit --pretty false",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "check": "pnpm -s run typecheck && pnpm -s run lint && pnpm -s run format",
    "wasm:build": "wasm-pack build ../../analyzer_wasm --target web --out-dir src/pkg"
  },
  "dependencies": {
    "@codemirror/commands": "^6.6.2",
    "@codemirror/lint": "^6.9.2",
    "@codemirror/state": "^6.4.1",
    "@codemirror/view": "^6.28.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@playwright/test": "^1.49.1",
    "@types/node": "^25.0.6",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.3.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.16.0",
    "vite": "^5.4.21",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.26.2"
}
